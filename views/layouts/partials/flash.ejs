<div class="container mt-3">
  <div class="row col-6 offset-3">
    <% if(success && success.length) { %>
    <div
      class="alert alert-success alert-dismissible fade show text-center"
      role="alert"
    >
      <%= success %>
      <button
        type="button"
        class="btn-close"
        data-bs-dismiss="alert"
        aria-label="Close"
      ></button>
    </div>
    <% } %>
  </div>
</div>


  <div class="row col-12">
    <% if(error &&error.length) { %>
      <div
      class="alert alert-warning alert-dismissible fade show text-center col-12"
      role="alert"
      >
        <%= error %>
        <button
          type="button"
          class="btn-close"
          data-bs-dismiss="alert"
          aria-label="Close"
        ></button>
      </div>
      <% } %>
  
</div>
